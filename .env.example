#
# Copy and rename this file to .env at root of this project.
#

# A common use case is to supply database creds via the environment. Edit settings.php
# like so:
#
# $databases['default']['default'] = [
#   'database' => getenv('MYSQL_DATABASE'),
#   'driver' => 'mysql',
#   'host' => getenv('MYSQL_HOSTNAME'),
#   'namespace' => 'Drupal\\Core\\Database\\Driver\\mysql',
#   'password' => getenv('MYSQL_PASSWORD'),
#   'port' => getenv('MYSQL_PORT'),
#   'prefix' => '',
#   'username' => getenv('MYSQL_USER'),
# ];
#
# Uncomment and populate as needed.
MYSQL_DATABASE=database_name
MYSQL_HOSTNAME=localhost
MYSQL_PASSWORD=password
#Port Number
# e.g. 8889
#  check your mysql process ```ps -ef | grep mysql``` or ```netstat -tlnp``` 
#   or tool stack info for mysql
MYSQL_PORT=PORT_NUMBER 
MYSQL_USER=mysql_username
HASH_SALT=random_hash #e.g. adlf093485lokljk34 -or- myTotallyRand0mh4sh

# Another common use case is to set Drush's --uri via environment.
# DRUSH_OPTIONS_URI=http://example.com
